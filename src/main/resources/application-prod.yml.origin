spring:
  datasource:
    url: jdbc:postgresql://localhost:5432/rms
    username: 
    password: 
    driver-class-name: org.postgresql.Driver
  jpa:
    properties: 
      hibernate:
        dialect: org.hibernate.dialect.PostgreSQLDialect
    hibernate:
      ddl-auto: update
    show-sql: true
logging:
  file:
    path: /log/rms.log
  logback:
    rollingpolicy:
      file-name-pattern: /log/rms-%d.gz
      max-history: 7
