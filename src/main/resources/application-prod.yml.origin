spring:
  sql:
    init:
      platform: postgresql
  datasource:
    url: jdbc:postgresql://localhost:5432/rms
    username: 
    password: 
    driver-class-name: org.postgresql.Driver
  jpa:
    properties: 
      hibernate:
        dialect: org.hibernate.dialect.PostgreSQLDialect
    hibernate:
      ddl-auto: update
    show-sql: true
